{
  "version": 2,
  "routes": [
    {
      "src": "/(.*\\.(jpg|jpeg|png|gif|webp|svg|ico))",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*\\.(css|js))",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*\\.(html|txt))",
      "headers": {
        "cache-control": "public, max-age=3600"
      }
    }
  ]
}
